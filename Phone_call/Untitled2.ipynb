   "source": [
    "for sel_date in tqdm([i.date() for i in\n",
    "                      reversed(pd.date_range(start=datetime.date(2021,9,10),\n",
    "                                             end=datetime.date(2021,12,10)))]):\n",
    "\n",
    "    (\n",
    "    spark.read.parquet('/datalab/sources/interconnect/parquet_logs/dt=%s' % sel_date)\n",
    "        .withColumn('a_number', F.expr(\"substring(a_number, 1)\"))\n",
    "        .withColumn('b_code', F.expr(\"substring(b_number, 1, 3)\"))\n",
    "        .filter(F.col('b_number').contains('998'))\n",
    "        .join(phone_uid, 'a_number', 'inner')\n",
    "        .select('uid')\n",
    "        .coalesce(1).write.mode('overwrite').csv('/user/content_analyst/result/RCMPGN-164/998_uid.csv', sep='\\t', header=False)\n",
    "    )\n",
    "    # заперсистим most_active и забьем фильтры\n",
    "    most_active = (\n",
    "        spark.read.parquet('/datalab/aggregates/uid_phone/most_active/dt=%s' % sel_date)\n",
    "            .select('uid', 'eip_phone_hashes', explode('call_center_numbers'))\n",
    "            .distinct('uid', )\n",
    "            .persist()\n",
    "    )\n",
    "    \n",
    "    (\n",
    "        most_active\n",
    "            .select('uid',  F.explode('sdp_phones').alias('phone'))\n",
    "            .distinct()\n",
    "            .write.parquet('tmp/65_calls_leasing/most_active/dt=%s/type=sdp' % sel_date)\n",
    "    )\n",
    "    \n",
    "    (\n",
    "        most_active\n",
    "            .select('uid',  F.explode('call_center_numbers').alias('phone'))\n",
    "            .distinct()\n",
    "            .write.parquet('tmp/65_calls_leasing/most_active/dt=%s/type=ccn' % sel_date)\n",
    "    )\n",
    "    \n",
    "    (\n",
    "        most_active\n",
    "            .select('uid',  F.explode('eip_phone_hashes').alias('hash'))\n",
    "            .distinct()\n",
    "            .join(\n",
    "                spark.read.parquet('/user/dmitriy.epifanov/export/phone_hash'),\n",
    "                'hash',\n",
    "                'inner'\n",
    "            )\n",
    "            .drop('hash')\n",
    "            .distinct()\n",
    "            .write.parquet('tmp/65_calls_leasing/most_active/dt=%s/type=eip' % sel_date)\n",
    "    )"
   ]
